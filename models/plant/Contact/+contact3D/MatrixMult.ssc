component MatrixMult

inputs
    R = zeros(3);
    x = 0;
    y = 0;
    z = 0;
  end

  outputs
    X = 0;
    Y = 0;
    Z = 0;
  end

  parameters
     transpose = 0; 
  end
  
  variables
     temp = [0;0;0]; 
  end

  equations
      
     if transpose 
        temp == R'*[x;y;z];
     else
        temp == R *[x;y;z];
     end
     X == temp(1);
     Y == temp(2);
     Z == temp(3);
     
  end
end
